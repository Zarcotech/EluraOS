#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/webOS/node_modules/.pnpm/parse-domain@8.2.2/node_modules/parse-domain/bin/node_modules:/root/webOS/node_modules/.pnpm/parse-domain@8.2.2/node_modules/parse-domain/node_modules:/root/webOS/node_modules/.pnpm/parse-domain@8.2.2/node_modules:/root/webOS/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/root/webOS/node_modules/.pnpm/parse-domain@8.2.2/node_modules/parse-domain/bin/node_modules:/root/webOS/node_modules/.pnpm/parse-domain@8.2.2/node_modules/parse-domain/node_modules:/root/webOS/node_modules/.pnpm/parse-domain@8.2.2/node_modules:/root/webOS/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/update.js" "$@"
else
  exec node  "$basedir/../../bin/update.js" "$@"
fi
